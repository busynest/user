  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/busynest"
  },
  "keywords": [
    "login",
    "pwa",
    "pwa-auth",
    "auth",
    "firebase",
    "icon",
    "drawer",
    "settings",
    "lit",
    "redux",
    "web components"
  ],
  "main": "./base/export/pwa-auth.js",
  "types": "./base/export/pwa-auth.d.ts",

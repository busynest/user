{
  "name": "pwa-auth",
  "author": "Modern Projects <jack.solutionbox@gmail.com>(https://www.contractorscentre.com)",
  "version": "0.0.15",
  "description": "User Management Component with 3 HTML Tags, and 1 Boilerplate Function to display user icon, sign up, login, log out, change account details, delete account.",
  "homepage": "https://www.inmostfire.com",
  "main": "pwa-auth.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/busynest/user"
  },
  "keywords": [
    "lit-html",
    "redux",
    "firebase",
    "user",
    "management",
    "drawer",
    "settings",
    "icon"
  ],
  "license": "BSD-3-Clause",
  "flat": true,
  "scripts": {
    "start": "polymer serve",
    "build": "polymer build --auto-base-path && workbox injectManifest workbox-config.js",
    "rollup": "rollup -c",
    "long": "rollup deploy/application-shell.js --format es --name 'ApplicationShell' --file application-shell.js",
    "build:static": "polymer build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "serve": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled"
  },
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "lit-element": "^2.4.0",
    "pwa-helpers": "^0.9.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.0.22",
    "@storybook/addon-essentials": "^6.0.22",
    "@storybook/addon-links": "^6.0.22",
    "@storybook/web-components": "^6.0.22",
    "lit-html": "^1.3.0",
    "polymer-cli": "^1.9.11",
    "polyserve": "^0.27.0",
    "prpl-server": "^1.4.2",
    "typescript": "^4.0.3"
  }
}
